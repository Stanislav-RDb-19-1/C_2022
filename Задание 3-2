#include <iostream>
#include <fstream>
using namespace std;

int main(){
ifstream test("output");
int n, num=1;
while(1){
test >> n;
cout << num << ". Число" << " " << n << endl;
}
return 0;
{
ifstream ttt("output.txt");
int a;
int b=1;
while(ttt >> a){
cout << b << ". Число" << a << endl;;
b++;
}
ttt.close();
return 0;
}
}
