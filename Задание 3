#include <iostream>
#include <fstream>
#include <math.h>
using namespace std;

int main(){

    ofstream output ("numbers.txt");

    if (output.is_open()){
       for(int num1 = 0 ; num1 <= 20 ; num1++)
        output<<pow(2, num1)<<endl;
}

    else cout << "Unable to open output file";

return 0;
}
