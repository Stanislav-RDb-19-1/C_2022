#include <iostream>
using namespace std;

int main(){
     
     int A,B,num1,num2;
     cout <<"Введите A" << endl;
     cin >> A;
     cout <<"Введите B" << endl;
     cin >> B;
     for(num1=0; num1<=B; num1++){
          for(num2=0; num2<=A; num2++)
               if(num1){
                    if(num2) cout << num1*num2 << "\t";
                    else cout << num1 << "\t";
               }
               else{
                    if(num2) cout << num2 << "\t";
                    else cout << ' ' << "\t";
               }
          cout << endl;
     }
     return 0;
}
