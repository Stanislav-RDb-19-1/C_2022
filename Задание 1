
#include <iostream>
using namespace std;

int main()
{
int A; A=10;
int B; B=10;
int num1;
int num2;
    for (num1=1; num1<=A; num1++)
{
    for (num2=1; num2<=B; num2++)
{
    cout << num1 * num2 << "  ";
    if (num2==10)
    cout << endl;
}
}
    return 0;
}
