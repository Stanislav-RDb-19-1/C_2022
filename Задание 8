#include <iostream>
using namespace std;

  char Rise(char A){
    return (char) (A-32);
}
  int Rise(int A){
    return A * 10;
}
int main(){
  char A; cin>>A;
    int num1;
      if(A >= '0' & A <= '9') {
         num1 = A - '0';
cout<<Rise(num1)<<endl;
}
      else if(A >= 'a' & A <= 'z')
cout<<Rise(A)<<endl;

return 0;
}
